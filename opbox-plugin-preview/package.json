    "@allegro/metrum-link": "5.3.0",
    "@allegro/metrum-select": "11.1.2",
    "@allegro/metrum-svelte-button": "0.1.0-beta.1",
    "@allegro/metrum-svelte-card": "0.1.0-beta.1",
    "@allegro/metrum-svelte-choice": "0.1.0-beta.1",
